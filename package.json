{
  "name": "po-extractor",
  "version": "0.0.1",
  "description": "extraction util for gettext-po i18n files",
  "main": "index.js",
  "scripts": {
    "test": "NODE_OPTIONS='--trace-warnings' nyc --reporter=text-summary mocha test/specs/**/*.test.js && nyc report --reporter=lcov",
    "examples": "scripts/examples run",
    "examples:build": "scripts/examples build",
    "errors": "scripts/errors info",
    "errors:build": "scripts/errors build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owings1/node-po-extractor.git"
  },
  "keywords": [
    "i18n",
    "gettext",
    "po",
    "translation",
    "internationalization",
    "internationalisation",
    "locale",
    "localization",
    "localisation"
  ],
  "author": "Doug Owings <doug@dougowings.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/owings1/node-po-extractor/issues"
  },
  "homepage": "https://github.com/owings1/node-po-extractor#readme",
  "devDependencies": {
    "chai": "^4.3.4",
    "diff": "^5.0.0",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "tmp": "^0.2.1"
  },
  "dependencies": {
    "@babel/core": "^7.14.8",
    "@babel/traverse": "^7.14.8",
    "chalk": "^4.1.2",
    "fs-extra": "^10.0.0",
    "gettext-parser": "^4.0.4",
    "console-utils-h": "github:owings1/node-console-utils-h",
    "globby": "^10.0.2"
  }
}
